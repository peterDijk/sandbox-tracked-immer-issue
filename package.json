{
  "name": "react-ts-rollup-starter",
  "version": "1.0.0",
  "main": "index.js",
  "module": "dist/bundle.js",
  "private": true,
  "author": "Peter van Dijk <petervandijk@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "npm run rollup:watch",
    "tsc": "tsc -p ./",
    "tsc:watch": "tsc -p ./ -w",
    "clean": "rm -rf dist/*",
    "rollup": "rollup -c rollup.config.js",
    "rollup:watch": "rollup -c rollup.config.js -w",
    "build": "npm run clean && npm run rollup"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.6.3",
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-replace": "^2.3.1",
    "@types/node": "^12.11.1",
    "@types/react": "^16.9.9",
    "@types/react-dom": "^16.9.2",
    "rollup": "^2.3.3",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-copy": "^3.1.0",
    "rollup-plugin-generate-html-template": "^1.5.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-livereload": "^1.1.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.5.0",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-typescript2": "^0.27.0",
    "rollup-plugin-uglify": "^6.0.3",
    "rollup-plugin-workbox": "^5.0.1",
    "tailwindcss": "^1.2.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "core-js": "^3.2.1",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "serialize-javascript": "^2.1.2"
  }
}
